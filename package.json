{
    "name": "svelte-crop-window",
    "version": "0.2.0",
    "description": "A Svelte component to crop images and videos with touch and mouse gestures",
    "homepage": "https://sabine.github.io/svelte-crop-window",
    "repository": {
        "type": "git",
        "url": "https://github.com/sabine/svelte-crop-window"
    },
    "author": "Sabine Schmaltz <info@sabineschmaltz.de>",
    "license": "MIT",
    "keywords": [
        "cropper",
        "crop window",
        "image crop",
        "video crop",
        "svelte",
        "svelte component"
    ],
    "scripts": {
        "dev": "vite dev --port 3000 --host",
        "gh-pages": "vite build && node gh-pages.cjs",
        "build": "svelte-kit sync && svelte-package && node clean-dependencies.js",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "devDependencies": {
        "@sveltejs/kit": "2.5.26",
        "@sveltejs/package": "2.3.4",
        "@sveltejs/adapter-static": "3.0.4",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.6",
        "svelte": "^4.2.19",
        "svelte-check": "^4.0.2",
        "svelte-highlight": "^7.7.0",
        "svelte-preprocess": "^6.0.2",
        "tslib": "^2.7.0",
        "typescript": "^5.6.2",
        "vite": "^5.4.4",
        "gh-pages": "^6.1.1"
    },
    "type": "module",
    "dependencies": {}
}
